<section class="page-section cta">
  <div class="container">
    <div class="row">
      <div class="col-xl-9 mx-auto">
        <div class="cta-inner text-center rounded">
          <h2 class="section-heading mb-5">
            <span class="section-heading-upper">{{ Model.ContentItem.Content.TitlePart.Title }}</span>
            <span class="section-heading-lower">{{ Model.ContentItem.Content.Store.Subtitle.Text }}</span>
          </h2>
          <ul class="list-unstyled list-hours mb-5 text-left mx-auto">
            {% for hours in Model.ContentItem.Content.BagPart.ContentItems %}
            <li class="list-unstyled-item list-hours-item d-flex">
              {{ hours.Workinghours.Weekday.Text }}
              <span class="ml-auto">{{ hours.TitlePart.Title }}</span>
            </li>
            {% endfor %}
          </ul>

            {% shape "Content__Contact", alias: "alias:contact" %}

        </div>
      </div>
    </div>
  </div>
</section>

{% shape "Content__Aboutus", alias: "alias:about-us" %}


